{
    "dataRoles": [
        {
            "name": "category",
            "kind": "Grouping",
            "displayName": "Categoría"
        },
        {
            "name": "values",
            "kind": "Measure",
            "displayName": "Valores"
        },
        {
            "name": "sparklineValues",
            "kind": "Measure",
            "displayName": "Valores Sparkline"
        }
    ],
    "dataViewMappings": [
        {
            "table": {
                "rows": {
                    "select": [
                        {
                            "for": {
                                "in": "category"
                            }
                        },
                        {
                            "for": {
                                "in": "values"
                            }
                        },
                        {
                            "for": {
                                "in": "sparklineValues"
                            }
                        }
                    ]
                }
            }
        }
    ],
    "objects": {
        "general": {
            "displayName": "General",
            "properties": {
                "textSize": {
                    "displayName": "Tamaño de texto",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "alternateRowColor": {
                    "displayName": "Color de filas alternadas",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }
        }
    },
    "privileges": []
}
