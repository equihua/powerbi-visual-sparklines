{
    "dataRoles": [
        {
            "displayName": "Category",
            "name": "mainCategory",
            "kind": "Grouping"
        },
        {
            "displayName": "Minigraph Axis",
            "name": "axis",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Minigraph Values",
            "name": "sparklines",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "mainCategory"
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 1000
                        }
                    }
                },
                "values": {
                    "group": {
                        "by": "axis",
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            },
                            {
                                "bind": {
                                    "to": "sparklines"
                                }
                            }
                        ]
                    }
                },
                "dataVolume": 4
            },
            "conditions": [
                {
                    "mainCategory": {
                        "max": 1
                    },
                    "axis": {
                        "max": 1
                    },
                    "measure": {
                        "max": 10
                    },
                    "sparklines": {
                        "max": 5
                    }
                }
            ]
        }
    ],
    "objects": {
        "general": {
            "displayName": "General",
            "properties": {
                "textSize": {
                    "displayName": "Tamaño de texto",
                    "type": {
                        "formatting": {
                            "fontSize": true
                        }
                    }
                },
                "alternateRowColor": {
                    "displayName": "Color de filas alternadas",
                    "type": {
                        "bool": true
                    }
                }
            }
        },
        "tableFeatures": {
            "displayName": "Características de tabla",
            "properties": {
                "enableFilters": {
                    "displayName": "Habilitar filtros",
                    "type": {
                        "bool": true
                    }
                },
                "enablePagination": {
                    "displayName": "Habilitar paginación",
                    "type": {
                        "bool": true
                    }
                },
                "enableSorting": {
                    "displayName": "Habilitar ordenamiento",
                    "type": {
                        "bool": true
                    }
                },
                "enableColumnResize": {
                    "displayName": "Habilitar redimensión de columnas",
                    "type": {
                        "bool": true
                    }
                },
                "initialPageSize": {
                    "displayName": "Tamaño de página inicial",
                    "type": {
                        "numeric": true
                    }
                }
            }
        },
        "sparklineColumn": {
            "displayName": "Minigráfico",
            "properties": {
                "chartType": {
                    "displayName": "Tipo de gráfico",
                    "type": {
                        "enumeration": [
                            {
                                "value": "line",
                                "displayName": "Línea"
                            },
                            {
                                "value": "bar",
                                "displayName": "Barras"
                            },
                            {
                                "value": "area",
                                "displayName": "Área"
                            },
                            {
                                "value": "pie",
                                "displayName": "Circular"
                            },
                            {
                                "value": "donut",
                                "displayName": "Anillo"
                            }
                        ]
                    }
                },
                "color": {
                    "displayName": "Color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                },
                "lineWidth": {
                    "displayName": "Grosor de línea",
                    "type": {
                        "numeric": true
                    }
                }
            }
        }
    },
    "sorting": {
        "implicit": {
            "clauses": [
                {
                    "role": "mainCategory",
                    "direction": 1
                }
            ]
        }
    },
    "supportsHighlight": true,
    "supportsLandingPage": true,
    "supportsEmptyDataView": false,
    "suppressDefaultTitle": true,
    "privileges": []
}