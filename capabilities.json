    "dataViewMappings": [
        {
            "categorical": {
                "categories": {
                    "for": {
                        "in": "mainCategory"
                    }
                },
                "values": {
                    "group": {
                        "by": "axis",
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            },
                            {
                                "bind": {
                                    "to": "sparkline"
                                }
                            }
                        ]
                    }
                },
                "dataVolume": 2
            },
            "conditions": [
                {
                    "mainCategory": {
                        "max": 1
                    },
                    "axis": {
                        "max": 1
                    },
                    "measure": {
                        "max": 10
                    },
                    "sparkline": {
                        "max": 5
                    }
                }
            ]
        }
    ],
