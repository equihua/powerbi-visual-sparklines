    "dataRoles": [
        {
            "displayName": "Category",
            "name": "mainCategory",
            "kind": "Grouping"
        },
        {
            "displayName": "Measure Data",
            "name": "measure",
            "kind": "Measure"
        },
        {
            "displayName": "Minigraph Axis",
            "name": "axis",
            "kind": "Grouping"
        },
        {
            "displayName": "Minigraph Data",
            "name": "sparkline",
            "kind": "Measure"
        }
